<h1>Listing places</h1>

<table>
  <tr>
    <th>Typecleaning</th>
    <th>Area</th>
    <th>Status</th>
    <th>Unit</th>
    <th>Building</th>
    <th>Floor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.typecleaning %></td>
    <td><%= place.area %></td>
    <td><%= place.status %></td>
    <td><%= place.unit %></td>
    <td><%= place.building %></td>
    <td><%= place.floor %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_place_path %>
